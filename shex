for D in $DEV
do
    if [ -e "$D" ]
    then
	CP="$D:$CP"
    else
	echo "Not found: $D" 1>&2
    fi
done

CP="$HOME/ASF/shapes/jena-shex/target/classes:$CP"
#echo $CP
java -cp "$CP" cmd.shex "$@"
